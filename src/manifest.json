{
"name":"gmail-crypt extension",
"version":"3",
"description":"Extension implementing GPG into Webmail (Gmail) using JS and the jsOpenPGP library",
"icons": {
"128":"gmail-crypt128.png"
},
"permissions":[
"https://mail.google.com/*",
"http://mail.google.com/*",
"https://google.com/*",
"http://google.com/*"
],
"background_page": "gmail-cryptBackground.html",
"content_scripts": [{
"matches":["https://mail.google.com/*","http://mail.google.com/*"],
"js": ["lib/jquery-1.6.2.min.js","lib/openpgp.js","gmail-cryptUtil.js","gmail-crypt.js"],
"run_at":"document_idle",
"all_frames":true
}],
"homepage_url":"http://github.com/seancolyer/gmail-crypt",
"update_url":"http://raw.github.com/seancolyer/gmail-crypt/master/build/updates.xml",
"options_page":"gmail-cryptOptions.html"
}
